SYNAPSE_MACAROON_SECRET_KEY="fH5#jK9!qW3$sD8&gL2*zX4%vB6^nM7"
SYNAPSE_FORM_SECRET="rT4@kP8!yU2$qI5&oW9*zE3%jH6^lO1"
